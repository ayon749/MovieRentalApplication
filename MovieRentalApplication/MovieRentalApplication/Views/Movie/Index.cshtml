@model List<MovieRentalApplication.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movie</h2>

<table class="table table-bordered table-hover" >
	<tr>
		<th>Movie</th>
	</tr>
	@foreach(var movie in Model)
	{
		<tr>
			<td>@movie.MovieName</td>
		</tr>
	}
</table>

